#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/axlcrawford/code/arca-chrome-extension/node_modules/.pnpm/webpack-extension-reloader@1.1.4_webpack@5.95.0_webpack-cli@5.1.4_/node_modules/webpack-extension-reloader/dist/node_modules:/Users/axlcrawford/code/arca-chrome-extension/node_modules/.pnpm/webpack-extension-reloader@1.1.4_webpack@5.95.0_webpack-cli@5.1.4_/node_modules/webpack-extension-reloader/node_modules:/Users/axlcrawford/code/arca-chrome-extension/node_modules/.pnpm/webpack-extension-reloader@1.1.4_webpack@5.95.0_webpack-cli@5.1.4_/node_modules:/Users/axlcrawford/code/arca-chrome-extension/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/axlcrawford/code/arca-chrome-extension/node_modules/.pnpm/webpack-extension-reloader@1.1.4_webpack@5.95.0_webpack-cli@5.1.4_/node_modules/webpack-extension-reloader/dist/node_modules:/Users/axlcrawford/code/arca-chrome-extension/node_modules/.pnpm/webpack-extension-reloader@1.1.4_webpack@5.95.0_webpack-cli@5.1.4_/node_modules/webpack-extension-reloader/node_modules:/Users/axlcrawford/code/arca-chrome-extension/node_modules/.pnpm/webpack-extension-reloader@1.1.4_webpack@5.95.0_webpack-cli@5.1.4_/node_modules:/Users/axlcrawford/code/arca-chrome-extension/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-extension-reloader/dist/webpack-extension-reloader-cli.js" "$@"
else
  exec node  "$basedir/../webpack-extension-reloader/dist/webpack-extension-reloader-cli.js" "$@"
fi
